{
  "schemaVersion": 1,
  "id": "cubic-addon",
  "version": "${version}",
  "name": "Cubic Addon",
  "description": "A Meteor Client addon that adds a Cubic button to multiplayer accounts.",
  "authors": [
    "Cube"
  ],
  "contact": {
    "repo": "https://github.com/Cube/Cube-s-meteor-add-on"
  },
  "icon": "assets/cubic/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.example.addon.CubicAddon"
    ]
  },
  "mixins": [
    "cubic-addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "100,100,255"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
